#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_conventional-changelog-cli@2.1.1@conventional-changelog-cli/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_conventional-changelog-cli@2.1.1@conventional-changelog-cli/cli.js" "$@"
  ret=$?
fi
exit $ret
